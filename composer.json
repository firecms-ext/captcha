{
  "name": "firecms-ext/captcha",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "captcha",
    "firecms",
    "hyperf"
  ],
  "description": "",
  "autoload": {
    "psr-4": {
      "FirecmsExt\\Captcha\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "tests"
    }
  },
  "require": {
    "php": ">=8.0",
    "hyperf/di": "^v2.0|^v3.0",
    "hyperf/framework": "^v2.0|^v3.0",
    "hyperf/http-server": "^v2.0|^v3.0",
    "hyperf/session": "^v2.0|^v3.0",
    "hyperf/validation": "^v2.0|^v3.0",
    "hyperf/cache": "^v2.0|^v3.0",
    "ext-gd": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": ">=7.0",
    "swoole/ide-helper": "^4.5"
  },
  "suggest": {
    "swow/swow": "Required to create swow components."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "hyperf": {
      "config": "FirecmsExt\\Captcha\\ConfigProvider"
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
